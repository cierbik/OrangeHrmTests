@NonCPS
def someMethod() {
    // Twoja logika
}

pipeline {
    agent any
    stages {
        stage('Run Cypress') {
            steps {
                script {
                    someMethod()  // wywołanie metody poza sandboxem
                    sh 'npx cypress run'  // uruchomienie testów Cypress
                }
            }
        }
    }
}
